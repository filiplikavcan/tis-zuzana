{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"repositories": [
		{
			"type": "path",
			"url": "../cleopas"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "3.5.0",
		"silverstripe/framework": "3.5.0",
		"silverstripe/reports": "3.5.0",
		"silverstripe/siteconfig": "3.5.0",

		"bratiask/cleopas": "*",
		"phpmailer/phpmailer": "^5.2"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.5.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
